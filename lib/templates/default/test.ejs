<% tables.forEach(function(tbl){ %>
[TableName("<%=tbl.name%>")]
<% if (tbl.pk!=null && tbl.pk.isAutoIncrement) { -%>
<% if (tbl.sequenceName==null) { -%>
[PrimaryKey("<%=tbl.pk.name%>")]
<%} -%>
<%} -%>
public partial class <%=tbl.name%>
{
    <%tbl.columns.forEach(function(col){%>
<% if (dbConfig.trackModifiedColumns) { _%>
    <%}-%>
    <%=col.name%>
    <%});%>
}
<%});%>